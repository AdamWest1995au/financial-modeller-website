{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/about",
      "destination": "/pages/about.html"
    },
    {
      "source": "/browse-models",
      "destination": "/pages/browse-models.html"
    },
    {
      "source": "/tailored-models",
      "destination": "/pages/tailored-models.html"
    },
    {
      "source": "/questionnaire",
      "destination": "/pages/questionnaire/questionnaire.html"
    },
    {
      "source": "/preview",
      "destination": "/pages/preview.html"
    },
    {
      "source": "/purchase",
      "destination": "/pages/purchase.html"
    },
    {
      "source": "/loading",
      "destination": "/pages/loading.html"
    },
    {
      "source": "/dashboard",
      "destination": "/pages/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/(.)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}